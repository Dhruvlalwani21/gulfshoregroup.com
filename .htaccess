ErrorDocument 404 /not-found.php

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^contact-us$ contact-us.php
RewriteRule ^about-us$ about-us.php
RewriteRule ^buy$ buy.php
RewriteRule ^sell$ sell.php 
RewriteRule ^settings$ settings.php
RewriteRule ^favorites/(.*)$ favorites.php?page=$1
RewriteRule ^mls-search$ mls-search.php
RewriteRule ^city/(.*)$ city.php?slug=$1
RewriteRule ^community/(.*)/(.*)/(.*)/(.*)$ community.php?slug=$1&type=$2&sort=$3&page=$4
 
RewriteRule ^terms$ terms.php
RewriteRule ^privacy$ privacy.php
  
RewriteRule ^property-details/(.*)/(.*)$ property-details.php?MLSNumber=$1&address=$2
RewriteRule ^homes-for-sale/(.*)/(.*)$ property-details.php?MLSNumber=$1&address=$2
